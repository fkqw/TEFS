TOOLDIR=/usr/local/GV-DataServer/sbin
CXX= g++

TESTOUTPUT=test


CCFLAGS=  -g  -std=c++0x

CCFLAGS += -I.
CCFLAGS += -I..
CCFLAGS += -I../common
CCFLAGS += -I../src

BASELIB =   -lpthread 
DLIB = -L../src -lds    
all:tool upload download del
tool:format clear cpuinfo status


upload:
	$(CXX) -o  PUT $(CCFLAGS) uploadtest.cpp $(DLIB) $(BASELIB)
download:
	$(CXX) -o  GET $(CCFLAGS) downloadtest.cpp $(DLIB) $(BASELIB)
del:
	$(CXX) -o  DEL $(CCFLAGS) deletetest.cpp $(DLIB) $(BASELIB)
format:
	$(CXX) -o  formattool $(CCFLAGS) format_file_system.cpp $(DLIB) $(BASELIB)
clear:
	$(CXX) -o  cleartool $(CCFLAGS) clear_file_system.cpp $(DLIB) $(BASELIB)
cpuinfo:
	$(CXX) -o  cpuinfo $(CCFLAGS) cputest.cpp $(DLIB) $(BASELIB)
atoi:
	 $(CXX) -o  atoi $(CCFLAGS) atoitest.cpp $(DLIB) $(BASELIB)
status:
	$(CXX) -o  showstatus $(CCFLAGS) showstatus.cpp $(DLIB) $(BASELIB)
install:
	mkdir -p $(DESTDIR)$(TOOLDIR)
	cp GET PUT DEL formattool cleartool cpuinfo showstatus $(DESTDIR)$(TOOLDIR)
clean:cleantool
	rm GET PUT DEL
cleantool:
	rm formattool cleartool cpuinfo showstatus
    
    



